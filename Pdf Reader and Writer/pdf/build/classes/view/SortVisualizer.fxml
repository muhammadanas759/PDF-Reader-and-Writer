<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" styleClass="firstFrame" stylesheets="@GeneralStyle.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SortVisualizerController">
   <center>
      <HBox fx:id="centerHBoxID" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
      </HBox>
   </center>
   <top>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="BubbleSort" accessibleRole="BUTTON" fill="SNOW" strokeType="OUTSIDE" strokeWidth="0.0" text="Bubble Sort" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Text>
            <Text fx:id="SelectionSort" accessibleRole="BUTTON" fill="SNOW" strokeType="OUTSIDE" strokeWidth="0.0" text="Selection Sort" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Text>
            <Text fx:id="InsertionSort" accessibleRole="BUTTON" fill="SNOW" strokeType="OUTSIDE" strokeWidth="0.0" text="Insertion Sort" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Text>
         </children></HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Text fill="SNOW" strokeType="OUTSIDE" strokeWidth="0.0" text="fast" textAlignment="CENTER" textOrigin="CENTER" wrappingWidth="30.0">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Text>
                  <Slider fx:id="sliderID" max="2000.0" min="100.0" prefHeight="30.0" prefWidth="140.0" value="400.0" />
                  <Text fill="SNOW" strokeType="OUTSIDE" strokeWidth="0.0" text="slow" textAlignment="CENTER" textOrigin="CENTER" wrappingWidth="30.0">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Text>
               </children>
            </HBox>
            <Button fx:id="SortButtonID" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#SortButtonClick" opacity="0.7" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: snow;" text="Sort" textFill="NAVY">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="GenerateButtonID" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#GenerateButtonClick" opacity="0.7" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: snow;" text="Generate" textFill="NAVY">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="BackButtonID" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#BackButtonClick" opacity="0.7" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: snow;" text="Back" textFill="NAVY">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
